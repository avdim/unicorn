  <!--  repo-->
  <depends>com.intellij.modules.json</depends>
  <depends>Git4Idea</depends>
  <extensions defaultExtensionNs="com.intellij">
    <codeInsight.lineMarkerProvider language="JSON" implementationClass="ru.tutu.repo.RepoGutter"/>
